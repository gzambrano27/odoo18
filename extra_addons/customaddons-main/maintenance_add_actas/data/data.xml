<odoo>
    <!-- Secuencia para Actas de Entrega -->
    <record id="seq_maintenance_acta_entrega" model="ir.sequence">
        <field name="name">Secuencia Acta de Entrega</field>
        <field name="code">maintenance.acta.entrega</field>
        <field name="prefix">GPS.ACT-ENTREGA-</field>
        <field name="padding">4</field> <!-- Número de dígitos del contador -->
        <field name="number_increment">1</field>
    </record>

    <!-- Secuencia para Actas de Recepción -->
    <record id="seq_maintenance_acta_recepcion" model="ir.sequence">
        <field name="name">Secuencia Acta de Recepción</field>
        <field name="code">maintenance.acta.recepcion</field>
        <field name="prefix">GPS.ACT-RECEPCION-</field>
        <field name="padding">4</field> <!-- Número de dígitos del contador -->
        <field name="number_increment">1</field>
    </record>
</odoo>
